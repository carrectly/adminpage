{
  "name": "adminpage",
  "version": "4.0.0",
  "description": "administrative website for car service business",
  "engines": {
    "node": "16.x"
  },
  "main": "index.js",
  "scripts": {
    "deploy": "script/deploy",
    "heroku-token": "NODE_ENV=development script/encrypt-heroku-auth-token.js",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "seed": "node server/db/seed.js",
    "seed:cars": "node server/db/seedCarTable.js",
    "start": "node server/index.js",
    "start-dev": "npm run build:dev & NODE_ENV='development' npm run start-server",
    "start-server": "nodemon server -e html,js,scss --ignore dist --ignore src",
    "build:dev": "webpack --config config/webpack.dev.js",
    "start-prod": "npm run build:prod & NODE_ENV='production' npm run start-server",
    "build:prod": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "update:list": "node ./updateCarCategories.js"
  },
  "author": "Andre Birkus",
  "license": "MIT",
